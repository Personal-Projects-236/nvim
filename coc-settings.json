{
	"cssmodules.camelCase": true,
	"cssmodules.hintName": "cssmodules",
	"emmet.includeLanguages": {
		"vue-html": "html",
		"javascript": "javascriptreact"
	},
	"emmet.excludeLanguages": [],
	"html-css-support.enabledLanguages": [
		"html",
		"vue",
		"blade",
		"htmldjango",
		"typescriptreact",
		"javascriptreact"
	],
	"markdown-preview-enhanced.enableScriptExecution": true,
	"coc.preferences.extensionUpdateCheck": "daily",
	"coc.preferences.formatOnSave": false,
	"coc.preferences.formatOnType": true,
	"ruff.autoFixOnSave": true,
	"workspace.workspaceFolderCheckCwd": false,
	"tsserver.tsdk": ".yarn/sdks/typescript/lib",
	"suggest.enablePreselect": false,
	"suggest.noselect": true,
	"explorer.quitOnOpen": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.keyMappings.global": {
		"<cr>": ["wait", "expanded?", "collapse", "expand"],
		"e": "open",
		"s": "open:split",
		"E": "open:vsplit"
	},
	"explorer.file.hiddenRules": {
		"filenames": ["node_modules", "notebook", "spell"]
	},
	"python.formatting.provider": "black",
	"htmldjango.enable": true,
	"htmldjango.completion.enable": true
}
